{
  "name": "@po.et/tslint-rules",
  "version": "1.4.0",
  "description": "Po.et TSlint rules",
  "main": "poet-tslint.json",
  "scripts": {
    "build": "rimraf rules && npm run compile",
    "compile": "tsc",
    "prepublish": "npm run build",
    "tslint:check": "tslint-config-prettier-check ./poet-tslint.json",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poetapp/tslint-rules.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/poetapp/tslint-rules/issues"
  },
  "dependencies": {
    "prettier": "1.14.3",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-immutable": "4.8.0",
    "tslint-plugin-prettier": "2.0.0"
  },
  "devDependencies": {
    "@types/node": "10.11.7",
    "rimraf": "2.6.2",
    "semantic-release": "15.9.17",
    "travis-deploy-once": "5.0.9",
    "typescript": "3.1.3"
  },
  "peerDependencies": {
    "typescript": "^2.1.0 || ^3.0.0"
  },
  "publishConfig": {
    "access": "public"
